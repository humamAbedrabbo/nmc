@model Inpatient

<div class="row">
    <div class="col-md-12">
        <div class="card-box">
            <div class="card-body">

                <div class="experience-box">
                    <ul class="experience-list">
                        <li>
                            <div class="experience-user">
                                <div class="before-circle"></div>
                            </div>
                            <div class="experience-content">
                                <div class="timeline-content">
                                    <a href="#" class="name">@_localizer["Created"]</a>
                                    <span class="time">@Model.CreatedOn</span>
                                </div>
                            </div>
                        </li>

                        <li hidden="@(!Model.AdmetOn.HasValue)">
                            <div class="experience-user">
                                <div class="before-circle"></div>
                            </div>
                            <div class="experience-content">
                                <div class="timeline-content">
                                    <a href="#" class="name">@_localizer["Admet"]</a>
                                    <span class="time">@Model.AdmetOn</span>
                                </div>
                            </div>
                        </li>

                        <li hidden="@(!Model.DischargedOn.HasValue)">
                            <div class="experience-user">
                                <div class="before-circle"></div>
                            </div>
                            <div class="experience-content">
                                <div class="timeline-content">
                                    <a href="#" class="name">@_localizer["Discharged"]</a>
                                    <span class="time">@Model.DischargedOn</span>
                                </div>
                            </div>
                        </li>


                    </ul>
                </div>

            </div>
        </div>
    </div>
</div> 